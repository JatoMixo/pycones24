#!/bin/bash
sed -i -E '/POT-Creation-Date:/d' i18n/contents+en.po i18n/contents+es.po i18n/contents+gl.po
msgattrib --sort-output --no-location --no-obsolete --no-wrap i18n/contents+es.po --output i18n/contents+es.po
msgattrib --sort-output --no-location --no-obsolete --no-wrap i18n/contents+gl.po --output i18n/contents+gl.po
msgattrib --sort-output --no-location --no-obsolete --no-wrap i18n/contents+en.po --output i18n/contents+en.po
